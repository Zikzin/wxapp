<!--pages/picker-view/picker-view.wxml-->


<view class="page">
    <view>{{seladdr}}--{{selcour}}--{{selclz}}</view>
    <picker-view indicator-style="height:60px" style="width:100%;height:100px;line-height:50px;text-align:center;" bindchange="change">
        <picker-view-column>
            <view wx:for="{{address}}" style="height:60px;">{{item}}</view>
        </picker-view-column>
        <picker-view-column>
            <view wx:for="{{course}}" style="height:60px;">{{item}}</view>
        </picker-view-column>
        <picker-view-column>
            <view wx:for="{{clz}}" style="height:60px;">{{item}}</view>
        </picker-view-column>
    </picker-view>
</view>